{
    "name": "nilportugues/schema-org",
    "description": "Schema.org mapping for JSON-LD generation",
    "type": "library",
    "homepage": "http://nilportugues.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nil Portugués Calderó",
            "email": "contact@nilportugues.com",
            "role": "Project Lead Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "schemaorg/schemaorg",
                "version": "2.2",
                "dist": {
                    "url": "https://github.com/schemaorg/schemaorg/archive/2.2.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "fabpot/php-cs-fixer": "^1.10"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "NilPortugues\\SchemaOrg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NilPortugues\\Tests\\SchemaOrg\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
